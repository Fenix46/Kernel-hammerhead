/include/ "../msm8974-camera-sensor-mtp.dtsi"
/include/ "../msm8974-leds.dtsi"

/include/ "msm8974-lge-panel.dtsi"
/include/ "msm8974-lge-input.dtsi"
/include/ "msm8974-lge-hdmi.dtsi"
/include/ "msm8974-lge-usb.dtsi"
/include/ "msm8974-lge-misc.dtsi"
/include/ "msm8974-lge-pm.dtsi"

/ {
	serial@f991d000 {
		compatible = "qcom,msm-lsuart-v14";
		reg = <0xf991d000 0x1000>;
		interrupts = <0 107 0>;
		status = "disabled";
	};
	uart10: uart@f9960000 { /*BLSP2, UART10 For A1 Bluetooth*/
		compatible = "qcom,msm-hsuart-v14";
		status = "ok";
		reg = <0xf9960000 0x1000>,
			<0xf9944000 0x19000>;
		reg-names = "core_mem", "bam_mem";
		interrupts = <0 116 0>, <0 239 0>;
		interrupt-names = "core_irq", "bam_irq";

		qcom,tx-gpio = <&msmgpio 53 0x00>;
		qcom,rx-gpio = <&msmgpio 54 0x00>;
		qcom,cts-gpio = <&msmgpio 55 0x00>;
		qcom,rfr-gpio = <&msmgpio 56 0x00>;

		qcom,bam-tx-ep-pipe-index = <6>;
		qcom,bam-rx-ep-pipe-index = <7>;

		qcom,msm-bus,name = "uart10";
		qcom,msm-bus,num-cases = <2>;
		qcom,msm-bus,active-only = <0>;
		qcom,msm-bus,num-paths = <1>;
		qcom,msm-bus,vectors-KBps =
				<84 512 0 0>,
				<84 512 500 800>;
	    };
        aliases{
               uart99 = &uart10;
        };

	/* label: i2c_0 (label defined in msm8974.dtsi) */
	i2c@f9967000 {
		cell-index = <0>;
		compatible = "qcom,i2c-qup";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0xf9967000 0x1000>;
		reg-names = "qup_phys_addr";
		interrupts = <0 105 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <400000>;
		qcom,i2c-src-freq = <50000000>;
		qcom,scl-gpio = <&msmgpio 84 0>;
		qcom,sda-gpio = <&msmgpio 83 0>;
		qcom,active-only;
		qcom,master-id = <84>;
	};

	i2c_1: i2c@f9923000 {
		cell-index = <54>;
		compatible = "qcom,i2c-qup";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0xf9923000 0x1000>;
		reg-names = "qup_phys_addr";
		interrupts = <0 95 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <100000>;
		qcom,i2c-src-freq = <50000000>;
		qcom,scl-gpio = <&msmgpio 3 0>;
		qcom,sda-gpio = <&msmgpio 2 0>;
		qcom,active-only;
		qcom,master-id = <86>;
	};

	/* label: i2c_2 (label defined in msm8974.dtsi) */
	i2c@f9924000 {
		cell-index = <2>;
		compatible = "qcom,i2c-qup";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0xf9924000 0x1000>;
		reg-names = "qup_phys_addr";
		interrupts = <0 96 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <400000>;
		qcom,i2c-src-freq = <50000000>;
		qcom,scl-gpio = <&msmgpio 7 0>;
		qcom,sda-gpio = <&msmgpio 6 0>;
		qcom,active-only;
		qcom,master-id = <86>;
	};
};

&spmi_bus {
	qcom,pm8941@0 {
		qcom,power-on@800 {
			qcom,pon_2 {
				qcom,disable = <1>;
			};
		};
	};
};
