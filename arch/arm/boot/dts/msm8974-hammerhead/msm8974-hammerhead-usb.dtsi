&usb_otg{
	compatible = "qcom,hsusb-otg";
	reg = <0xf9a55000 0x400>;
};

&usb3{
	compatible = "qcom,dwc-usb3-msm";
	reg = <0xf9200000 0xfc000>,
		<0xfd4ab000 0x4>;
	qcom,dwc-usb3-msm-adc-low-threshold = <100>;
	qcom,dwc-usb3-msm-adc-high-threshold = <200>;
};
